WIDTH=8;
DEPTH=65536;

ADDRESS_RADIX=UNS;
DATA_RADIX=BIN;

CONTENT BEGIN
	[0..65535]	:	00000000;
	0	:	00001001;	--mv   r1, #50
	1	:	00110010;
	2	:	01000001;	--ld    r1, r1
	3	:	00000001;
	4	:	11110001;	--in 01
	5	:	00000100;	--mv r4, r7
	6	:	00000111;
	7	:	00110001;	--mul r1, r4
	8	:	00000100;
	9	:	00001001;	--mv r1, #52
	10	:	00110100;
	11	:	01010110;	--st r6, r1
	12	:	00000001;
	13	:	00001001;	--mv   r1, #52
	14	:	00110100;
	15	:	01000001;	--ld    r1, r1
	16	:	00000001;
	17	:	00000111;	--mv r7, r1
	18	:	00000001;
	19	:	11110100;	--out 00
	20	:	11111111;	--halt
	50	:	00001100;
END;